{
  "expo": {
    "name": "Brewtopia",
    "slug": "brewtopia",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "brewtopia",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "icon": "./assets/images/iconcafe.png",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.brewtopia.app",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Cho phép Brewtopia sử dụng vị trí của bạn để tìm các quán cà phê gần đây và chỉ đường.",
        "NSLocationAlwaysUsageDescription": "Cho phép Brewtopia sử dụng vị trí của bạn để tìm các quán cà phê gần đây và chỉ đường.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Cho phép Brewtopia sử dụng vị trí của bạn để tìm các quán cà phê gần đây và chỉ đường.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDmwLRVHrEYt9IkLZlf4ylndLQpPpF889w"
      }
    },
    "android": {
      "package": "com.brewtopia.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/iconcafe.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_BACKGROUND_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDmwLRVHrEYt9IkLZlf4ylndLQpPpF889w"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/iconcafe.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/iconcafe.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Cho phép Brewtopia sử dụng vị trí của bạn để tìm các quán cà phê gần đây và chỉ đường."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0a243565-20a0-4dfc-a872-4c92055b717d"
      }
    },
    "owner": "khoiawesome"
  }
}
